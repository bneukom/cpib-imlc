program listSum()
global
fun sum(in copy l:[int]) returns var r:int
do
	if length l == 0 do
		r init := 0
	else
		r := head l + sum(tail l)
	endif
endfun;

var l:[int];
var sum2:int;
var sum:int
do
	l init := [1,2,3,4,5,6,7,8];
	sum2 init := head head l;
	sum init := sum(l)
endprogram

