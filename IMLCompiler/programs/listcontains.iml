program listContains()
global

fun contains(in copy l:[int], in copy i) returns var r:boolean
do
	if length l == 0 do
		r init := false
	else
		r := head l == i || contains(tail l, i)
	endif
endfun;


var l:[int];
var i:int;
var c:bool
do
	l init := [1,2,3,4,5,6,7,8];
	i init := 6;
	c init := contains(i)
endprogram
