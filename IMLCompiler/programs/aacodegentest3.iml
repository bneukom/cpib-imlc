program aacodegentest3() 
global 
 var v:int;
 var l:[int];
 proc test(inout copy var p:int, in copy var v:int)
 local
  var c:int
 do
  p := p * v
 endproc;
 proc test2(inout copy var l:[int])
 do
  l := 0 :: l
 endproc
do 
