program intDiv(in const progInput:int)
global
 proc divide(in copy m:int, in copy n:int, out ref var q:int, out ref var r:int)
 global in const x
 do
  q init:= 0;
  r init:= m;
  while r >= n do
   q := q + 1;
   r := r - n
  endwhile
 endproc;
 var m:int;
 var n:int;
 var q:int;
 var r:int
do
 m init := 3;
 input m;
 input n init;
 call divide(m, n, q init, r init);
 output q;
 output r
endprogram