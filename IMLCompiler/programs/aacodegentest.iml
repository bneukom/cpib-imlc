program aacodegentest() 
global 
fun isPrime(in copy const p:int) returns var b:bool
global
local
	var c:int
do
	c init := 2;
	
	if p > 1 do
		b init := true;
		while c < p do
			if p mod c == 0 do
				b := false
			else
				skip
			endif;
			c := c + 1
		endwhile
	else
		b init := false
	endif
endfun;

fun sum(in copy const l:[int]) returns var r:int
local 
var x:int
do
	if length l == 0 do
		r init := 0
	else
		r := head l + sum(tail l)
	endif
endfun;

var l:[int];
var numPrimes:int
do 
 debugin numPrimes init;
 l init := { x | x from 0 to numPrimes when isPrime(x) };
 debugout l;
 debugout head tail tail tail tail l;
 debugout sum(l)
endprogram